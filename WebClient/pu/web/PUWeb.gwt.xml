<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='puweb'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.xml.XML" />

  <inherits name='com.google.gwt.user.theme.clean.Clean'/>

  <generate-with class="pu.web.generator.ResourceGenerator">
    <when-type-assignable class="pu.web.shared.ResourceBundle" />
  </generate-with>

  <!-- Other module inherits                                      -->
  <inherits name='com.googlecode.gwtgl.binding'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='pu.web.client.PUWeb'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
